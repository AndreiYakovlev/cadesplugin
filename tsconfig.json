{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": true,
        "alwaysStrict": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "lib": [
            "es5", "es2015.promise", "es2015.iterable", "dom"
        ]
    }
}